    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-bootstrap-table": "4.3.1",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-grid-layout": "^0.16.6",
